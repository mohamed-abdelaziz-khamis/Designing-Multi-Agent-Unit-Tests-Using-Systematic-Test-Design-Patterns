<?xml version="1.0"?>

<TestDesignPattern>

	<SuccessfulScenario>		
		<Offered_Sub_Request_Title>sub-request-title</Offered_Sub_Request_Title> 
		<Sub_Result>sub-result</Sub_Result>
		<Main_Request_Title>request-title</Main_Request_Title>
		<Sub_Request_Title>sub-request-title</Sub_Request_Title>
		<Waker_Behaviour_Period>60000</Waker_Behaviour_Period>
		<Subscription_REQUEST_Performative>REQUEST</Subscription_REQUEST_Performative>
		<Subscription_REQUEST_ConversationID>service-mediating</Subscription_REQUEST_ConversationID>
		<Subscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Subscription_ACCEPT_PROPOSAL_Performative>
		<Subscription_ACCEPT_PROPOSAL_Content>subscription-accepted</Subscription_ACCEPT_PROPOSAL_Content>
		<Subscription_REFUSE_Performative>REFUSE</Subscription_REFUSE_Performative>
		<Subscription_REFUSE_Content>subscription-refused</Subscription_REFUSE_Content>
		<Unsubscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Unsubscription_ACCEPT_PROPOSAL_Performative>
		<Unsubscription_ACCEPT_PROPOSAL_Content>unsubscription-accepted</Unsubscription_ACCEPT_PROPOSAL_Content>
		<Unsubscription_REFUSE_Performative>REFUSE</Unsubscription_REFUSE_Performative>
		<Unsubscription_REFUSE_Content>unsubscription-refused</Unsubscription_REFUSE_Content>
		<Sub_Result_REQUEST_Performative>REQUEST</Sub_Result_REQUEST_Performative>
		<Sub_Result_REQUEST_ConversationID>service-mediating</Sub_Result_REQUEST_ConversationID>
		<Sub_Result_INFORM_Performative>INFORM</Sub_Result_INFORM_Performative>
		<Sub_Result_INFORM_ConversationID>service-mediating</Sub_Result_INFORM_ConversationID>		
	</SuccessfulScenario>
	
	<ExceptionalScenario>
		<Offered_Sub_Request_Title>sub-request-title</Offered_Sub_Request_Title> 
		<Sub_Result>sub-result</Sub_Result>
		<Main_Request_Title>request-title</Main_Request_Title>
		<Sub_Request_Title>other-sub-request-title</Sub_Request_Title>
		<Waker_Behaviour_Period>60000</Waker_Behaviour_Period>
		<Subscription_REQUEST_Performative>REQUEST</Subscription_REQUEST_Performative>
		<Subscription_REQUEST_ConversationID>service-mediating</Subscription_REQUEST_ConversationID>
		<Subscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Subscription_ACCEPT_PROPOSAL_Performative>
		<Subscription_ACCEPT_PROPOSAL_Content>subscription-accepted</Subscription_ACCEPT_PROPOSAL_Content>
		<Subscription_REFUSE_Performative>REFUSE</Subscription_REFUSE_Performative>
		<Subscription_REFUSE_Content>subscription-refused</Subscription_REFUSE_Content>
		<Unsubscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Unsubscription_ACCEPT_PROPOSAL_Performative>
		<Unsubscription_ACCEPT_PROPOSAL_Content>unsubscription-accepted</Unsubscription_ACCEPT_PROPOSAL_Content>
		<Unsubscription_REFUSE_Performative>REFUSE</Unsubscription_REFUSE_Performative>
		<Unsubscription_REFUSE_Content>unsubscription-refused</Unsubscription_REFUSE_Content>
		<Sub_Result_REQUEST_Performative>REQUEST</Sub_Result_REQUEST_Performative>
		<Sub_Result_REQUEST_ConversationID>service-mediating</Sub_Result_REQUEST_ConversationID>
		<Sub_Result_INFORM_Performative>INFORM</Sub_Result_INFORM_Performative>
		<Sub_Result_INFORM_ConversationID>service-mediating</Sub_Result_INFORM_ConversationID>
	</ExceptionalScenario>
	
</TestDesignPattern>