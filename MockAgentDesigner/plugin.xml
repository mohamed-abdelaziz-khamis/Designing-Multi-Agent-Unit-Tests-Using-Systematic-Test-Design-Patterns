<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension point="org.eclipse.ui.actionSets">
      
      <actionSet label="Mock Agent Designer Action Set" visible="true" id="MockAgentDesigner.actionSet">
        
         <menu label="Mock Agent Designer" id="mockAgentDesignerMenu">                        
            <separator name="mockAgentDesignerGroup">
            </separator>            
            <separator name="designPatternCategoryGroup">
            </separator>
            <separator name="designPatternGroup">
            </separator>            
            <separator name="agentRoleGroup">
            </separator>
            <separator name="mockInteractingRoleGroup">
            </separator>
            <separator name="mockAgentTemplateGroup">
            </separator>
         </menu>
         
         <action
               label="Mock Agent Designer"
               icon="icons/sample.gif"
               class="mockagentdesigner.actions.MockAgentDesigner"              
               menubarPath="mockAgentDesignerMenu/mockAgentDesignerGroup"
               id="mockagentdesigner.actions.MockAgentDesigner">
         </action>
         
         <action
               label="Design Pattern Category"
               class="mockagentdesigner.actions.DesignPatternCategory"
               menubarPath="mockAgentDesignerMenu/designPatternCategoryGroup"
               id="mockagentdesigner.actions.DesignPatternCategory">
         </action>
         
         <action
               label="Design Pattern"
               class="mockagentdesigner.actions.DesignPattern"
               menubarPath="mockAgentDesignerMenu/designPatternGroup"
               id="mockagentdesigner.actions.DesignPattern">
         </action>
         
         <action
               label="Agent Role"
               class="mockagentdesigner.actions.AgentRole"
               menubarPath="mockAgentDesignerMenu/agentRoleGroup"
               id="mockagentdesigner.actions.AgentRole">
         </action>
         
         <action
               label="Mock Interacting Role"
               class="mockagentdesigner.actions.MockInteractingRole"
               menubarPath="mockAgentDesignerMenu/mockInteractingRoleGroup"
               id="mockagentdesigner.actions.MockInteractingRole">
         </action>
         
         <action
               label="Mock Agent Template"
               class="mockagentdesigner.actions.MockAgentTemplate"
               menubarPath="mockAgentDesignerMenu/mockAgentTemplateGroup"
               id="mockagentdesigner.actions.MockAgentTemplate">
         </action>

      </actionSet>
      
   </extension>

</plugin>
