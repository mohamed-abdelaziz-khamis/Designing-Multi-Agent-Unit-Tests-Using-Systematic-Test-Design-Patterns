<?xml version="1.0"?>

<TestDesignPattern>

	<SuccessfulScenario>
		<Offered_Service_Title>service-title</Offered_Service_Title>
		<Offered_Service_Price>100</Offered_Service_Price>
		<Requested_Service_Title>service-title</Requested_Service_Title>
		<REQUEST_Performative>REQUEST</REQUEST_Performative> 
		<REQUEST_ConversationID>service-broking</REQUEST_ConversationID>
		<Service_Description_Type_Key>Service_Description_Type</Service_Description_Type_Key>
		<Service_Description_Name_Key>Service_Description_Name</Service_Description_Name_Key>
		<Subscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Subscription_ACCEPT_PROPOSAL_Performative>
		<Subscription_ACCEPT_PROPOSAL_Content>subscription-accepted</Subscription_ACCEPT_PROPOSAL_Content>
		<Subscription_REFUSE_Performative>REFUSE</Subscription_REFUSE_Performative>
		<Subscription_REFUSE_Content>subscription-refused</Subscription_REFUSE_Content>
		<Unsubscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Unsubscription_ACCEPT_PROPOSAL_Performative>
		<Unsubscription_ACCEPT_PROPOSAL_Content>unsubscription-accepted</Unsubscription_ACCEPT_PROPOSAL_Content>
		<Unsubscription_REFUSE_Performative>REFUSE</Unsubscription_REFUSE_Performative>
		<Unsubscription_REFUSE_Content>unsubscription-refused</Unsubscription_REFUSE_Content>
		<CFP_Performative>CFP</CFP_Performative>
		<CFP_ConversationID>service-broking</CFP_ConversationID>
		<PROPOSE_Performative>PROPOSE</PROPOSE_Performative>
		<PROPOSE_ConversationID>service-broking</PROPOSE_ConversationID>
		<ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</ACCEPT_PROPOSAL_Performative>
		<ACCEPT_PROPOSAL_ConversationID>service-broking</ACCEPT_PROPOSAL_ConversationID> 
		<INFORM_Performative>INFORM</INFORM_Performative>
		<INFORM_ConversationID>service-broking</INFORM_ConversationID> 
	</SuccessfulScenario>
	
	<ExceptionalScenario>
		<Offered_Service_Title>service-title</Offered_Service_Title>
		<Offered_Service_Price>100</Offered_Service_Price>
		<Requested_Service_Title>other-service-title</Requested_Service_Title>
		<REQUEST_Performative>REQUEST</REQUEST_Performative> 
		<REQUEST_ConversationID>service-broking</REQUEST_ConversationID>
		<Service_Description_Type_Key>Service_Description_Type</Service_Description_Type_Key>
		<Service_Description_Name_Key>Service_Description_Name</Service_Description_Name_Key>
		<Subscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Subscription_ACCEPT_PROPOSAL_Performative>
		<Subscription_ACCEPT_PROPOSAL_Content>subscription-accepted</Subscription_ACCEPT_PROPOSAL_Content>
		<Subscription_REFUSE_Performative>REFUSE</Subscription_REFUSE_Performative>
		<Subscription_REFUSE_Content>subscription-refused</Subscription_REFUSE_Content>
		<Unsubscription_ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</Unsubscription_ACCEPT_PROPOSAL_Performative>
		<Unsubscription_ACCEPT_PROPOSAL_Content>unsubscription-accepted</Unsubscription_ACCEPT_PROPOSAL_Content>
		<Unsubscription_REFUSE_Performative>REFUSE</Unsubscription_REFUSE_Performative>
		<Unsubscription_REFUSE_Content>unsubscription-refused</Unsubscription_REFUSE_Content>
		<CFP_Performative>CFP</CFP_Performative>
		<CFP_ConversationID>service-broking</CFP_ConversationID>
		<PROPOSE_Performative>PROPOSE</PROPOSE_Performative>
		<PROPOSE_ConversationID>service-broking</PROPOSE_ConversationID>
		<ACCEPT_PROPOSAL_Performative>ACCEPT_PROPOSAL</ACCEPT_PROPOSAL_Performative>
		<ACCEPT_PROPOSAL_ConversationID>service-broking</ACCEPT_PROPOSAL_ConversationID> 
		<INFORM_Performative>INFORM</INFORM_Performative>
		<INFORM_ConversationID>service-broking</INFORM_ConversationID> 
	</ExceptionalScenario>
</TestDesignPattern>